%h1 Listing users
%table
  %tr
    %th id
    %th Name
    %th Email
    %th Enabled
  - @users.each do |user|
    %tr
      %td= user[:id]
      %td= user[:name]
      %td= user[:email]
      %td= user[:enabled]
      %td= link_to 'Show', user
      %td= link_to 'Edit', edit_user_path(user)
      %td= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete
%br/
= link_to 'New Tenant', new_user_path